    "mockdate": "^3.0.5",
    "readline-sync": "^1.4.10",
    "simple-git": "^3.19.1",
    "web3": "^4.0.3"
  },
  "main": "hardhat.config.ts",
  "directories": {
    "test": "test"
  },
  "packageManager": "yarn@3.6.0"
}
